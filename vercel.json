{
  "buildCommand": "pip install -r requirements.txt",
  "framework": "fastapi",
  "functions": {
    "api/index.py": {
      "runtime": "python3.12",
      "memory": 1024,
      "timeout": 60
    }
  },
  "env": {
    "OPENROUTER_API_KEY": "@openrouter_api_key",
    "OPENROUTER_BASE_URL": "@openrouter_base_url",
    "SUPABASE_URL": "@supabase_url",
    "SUPABASE_KEY": "@supabase_key"
  }
}
